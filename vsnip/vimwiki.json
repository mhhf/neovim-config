      "```",
      "",
      "<!-- name: $1 -->",
      "```",
      "```"
    ]
  },
  "python": {
    "prefix": "python",
    "body": [
      "<!-- target: $1 -->",
      "```python",
      "$2",
      "```",
      "",
      "<!-- name: $1 -->",
      "```",
      "```"
    ]
  },
  "haskell": {
    "prefix": "haskell",
    "body": [
      "<!-- target: $1 -->",
